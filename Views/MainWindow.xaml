<Window x:Class="ItemsSourceBindEnum.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="{Binding Title}" Height="350" Width="525" >
    <StackPanel>
        <ComboBox ItemsSource="{Binding Weeks}" Text="{Binding SelectWeek}" Width="200" Height="25" Margin="20"/>
        <TextBlock Text="{Binding SelectWeek}" Width="200" Height="25" Background="Transparent" Foreground="Black" Margin="20"/>
    </StackPanel>
</Window>
